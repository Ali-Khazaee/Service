{
    "globals": { "Config": true },
    "extends": "standard",
    "rules":
    {
        "indent": [ "error", 4, { "CallExpression": { "arguments": 0 }, "SwitchCase": 1 } ],
        "brace-style": [ "error", "allman" ],
        "space-before-function-paren": [ "error", "never" ],
        "curly": [ "error", "multi-or-nest" ]
    }
}
